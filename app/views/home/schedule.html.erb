                <center><i class="material-icons"style="font-size:24px">file_download</i></center>
            </div>
        </div>
        <div class="col s12 m4"style="cursor:pointer">
            <div id="titled3"style="width:100%;background:rgba(0,0,0,0.8);color:white;padding-top:7px;padding-bottom:7px;margin:0px">
