            </div>
            <div id="download3"style="width:100%;height:40vh;padding-top:50%;color:black;background:rgba(255,255,0,0.7)">
                <center><i class="material-icons"style="font-size:24px">file_download</i></center>
            </div>
        </div>

</div>
<script>
function appear(id)
{
    document.getElementById('content'+id).style.display="none";
    document.getElementById('download'+id).style.display="block";
    $('#content'+id).delay(1000).fadeIn();
    $('#download'+id).delay(1000).slideUp();
}
</script>
